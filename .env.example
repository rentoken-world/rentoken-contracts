# Rentoken Contracts Environment Configuration
# Copy this file to .env and fill in your actual values

# RPC Configuration
RPC_URL=https://eth-sepolia.public.blastapi.io
CHAIN_ID=11155111

# Etherscan Configuration (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Private Keys (keep these secure and never commit to git)
PRIVATE_KEY=your_private_key_here
DEPLOYER_PRIVATE_KEY=your_deployer_private_key_here

# Contract Addresses (will be populated after deployment)
KYC_ORACLE_ADDR=0x0000000000000000000000000000000000000000
PROPERTY_ORACLE_ADDR=0x0000000000000000000000000000000000000000
RENT_TOKEN_IMPL_ADDR=0x0000000000000000000000000000000000000000
SANCTION_ORACLE_ADDR=0x0000000000000000000000000000000000000000
SERIES_FACTORY_ADDR=0x0000000000000000000000000000000000000000
USDC_ADDR=0x0000000000000000000000000000000000000000

# Test Property IDs (will be populated after property registration)
SEPOLIA_CASE2_PROPERTY1_ID=0
SEPOLIA_CASE2_PROPERTY2_ID=0
SEPOLIA_CASE2_PROPERTY3_ID=0

# Test Series Addresses (will be populated after series creation)
SEPOLIA_CASE2_SERIES1_ADDR=0x0000000000000000000000000000000000000000
SEPOLIA_CASE2_SERIES2_ADDR=0x0000000000000000000000000000000000000000
SEPOLIA_CASE2_SERIES3_ADDR=0x0000000000000000000000000000000000000000

# Test Token Symbols (will be populated after token creation)
SEPOLIA_CASE2_TOKEN1_SYMBOL=
SEPOLIA_CASE2_TOKEN2_SYMBOL=
SEPOLIA_CASE2_TOKEN3_SYMBOL=

# Optional: Gas Configuration
GAS_LIMIT=3000000
GAS_PRICE=20000000000  # 20 gwei

# Optional: Fork Configuration for local development
FORK_URL=https://eth-sepolia.public.blastapi.io
FORK_BLOCK_NUMBER=latest
